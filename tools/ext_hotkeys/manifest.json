{
  "manifest_version": 3,
  "name": "Machete Vault - Hotkeys Manager",
  "version": "1.0",
  "description": "hotkeys browser extension",
  "permissions": ["commands", "tabs", "activeTab", "scripting"],
  "background": {
    "service_worker": "background.js"
  },
  "commands": {
    "paste-data-1": {
      "suggested_key": {
        "default": "Ctrl+Shift+1"
      },
      "description": "first data option"
    },
    "paste-data-2": {
      "suggested_key": {
        "default": "Ctrl+Shift+2"
      },
      "description": "second data option"
    },
    "paste-data-3": {
      "suggested_key": {
        "default": "Ctrl+Shift+3"
      },
      "description": "third data option"
    },
    "open-predefined-tab": {
      "suggested_key": {
        "default": "Ctrl+Shift+N"
      },
      "description": "open a new tab with predefined URLs"
    }
  }
}